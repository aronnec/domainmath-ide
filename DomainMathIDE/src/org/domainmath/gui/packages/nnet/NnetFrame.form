    <Component class="javax.swing.JButton" name="trainButton">
      <Properties>
        <Property name="text" type="java.lang.String" value="Train"/>
        <Property name="toolTipText" type="java.lang.String" value="&lt;html&gt;&lt;div class=&quot;defun&quot;&gt;&#xa;&lt;div class=&quot;defun&quot;&gt;&#xa;&lt;p class=&quot;functionfile&quot;&gt; Function File: &lt;b&gt;[&lt;/b&gt;&lt;var&gt;net&lt;/var&gt;]&lt;var&gt;&#xa;= train &lt;/var&gt;(&lt;var&gt;MLPnet,mInputN,mOutput,[],[],VV&lt;/var&gt;)&lt;var&gt;&lt;a&#xa; name=&quot;index-g_t_005b-1&quot;&gt;&lt;/a&gt;&lt;/var&gt;&lt;br&gt;&#xa;&lt;/p&gt;&#xa;&lt;blockquote&gt;&#xa;  &lt;p&gt;A neural feed-forward network will be trained with &lt;code&gt;train&lt;/code&gt;&#xa;  &lt;/p&gt;&#xa;  &lt;pre class=&quot;example&quot;&gt;          [net,tr,out,E] = train(MLPnet,mInputN,mOutput,[],[],VV);&lt;br&gt;&lt;/pre&gt;&#xa;  &lt;p class=&quot;noindent&quot;&gt; &lt;/p&gt;&#xa;  &lt;pre class=&quot;example&quot;&gt;          left side arguments:&lt;br&gt;            &lt;span&#xa; style=&quot;font-weight: bold;&quot;&gt;net&lt;/span&gt;: the trained network of the net structure &lt;code&gt;MLPnet&lt;/code&gt;&#xa;  &lt;/pre&gt;&#xa;  &lt;p class=&quot;noindent&quot;&gt; &lt;/p&gt;&#xa;  &lt;pre class=&quot;example&quot;&gt;          right side arguments:&lt;br&gt;            &lt;span&#xa; style=&quot;font-weight: bold;&quot;&gt;MLPnet&lt;/span&gt; : the untrained network, created with &lt;code&gt;newff&lt;/code&gt;&lt;br&gt;            &lt;span&#xa; style=&quot;font-weight: bold;&quot;&gt;mInputN&lt;/span&gt;: normalized input matrix&lt;br&gt;            &lt;span&#xa; style=&quot;font-weight: bold;&quot;&gt;mOutput&lt;/span&gt;: output matrix (normalized or not)&lt;br&gt;            &lt;span&#xa; style=&quot;font-weight: bold;&quot;&gt;[]&lt;/span&gt;     : unused parameter&lt;br&gt;            &lt;span&#xa; style=&quot;font-weight: bold;&quot;&gt;[]&lt;/span&gt;     : unused parameter&lt;br&gt;            &lt;span&#xa; style=&quot;font-weight: bold;&quot;&gt;VV&lt;/span&gt;     : validize structure&lt;br&gt;&lt;/pre&gt;&#xa;&lt;/blockquote&gt;&#xa;&lt;/div&gt;&#xa;&lt;/div&gt;&lt;/html&gt;&#xa;"/>
