    <Component class="javax.swing.JButton" name="newButton">
      <Properties>
        <Property name="text" type="java.lang.String" value="New Network"/>
        <Property name="toolTipText" type="java.lang.String" value="&lt;html&gt;&lt;div class=&quot;defun&quot;&gt;&#xa;&lt;p class=&quot;functionfile&quot;&gt; Function File: &lt;var&gt;net&lt;/var&gt;&#xa;&lt;b&gt;=&lt;/b&gt;&lt;var&gt; &lt;span style=&quot;font-weight: bold;&quot;&gt;newff&lt;/span&gt;&#xa;&lt;/var&gt;(&lt;var&gt;Pr,ss,trf,btf,blf,pf&lt;/var&gt;)&lt;var&gt;&lt;a&#xa; name=&quot;index-g_t_003d-1&quot;&gt;&lt;/a&gt;&lt;/var&gt;&lt;br&gt;&#xa;&lt;/p&gt;&#xa;&lt;blockquote&gt;&#xa;  &lt;p&gt;&lt;code&gt;newff&lt;/code&gt; create a feed-forward&#xa;backpropagation network &lt;/p&gt;&#xa;  &lt;pre class=&quot;example&quot;&gt;         &lt;span&#xa; style=&quot;font-weight: bold;&quot;&gt; Pr&lt;/span&gt; - R x 2 matrix of min and max values for R input elements&lt;br&gt;          &lt;span&#xa; style=&quot;font-weight: bold;&quot;&gt;Ss&lt;/span&gt; - 1 x Ni row vector with size of ith layer, for N layers&lt;br&gt;          &lt;span&#xa; style=&quot;font-weight: bold;&quot;&gt;trf&lt;/span&gt; - 1 x Ni list with transfer function of ith layer,&lt;br&gt;                default = &quot;tansig&quot;&lt;br&gt;          &lt;span&#xa; style=&quot;font-weight: bold;&quot;&gt;btf&lt;/span&gt; - Batch network training function,&lt;br&gt;                default = &quot;trainlm&quot;&lt;br&gt;          &lt;span&#xa; style=&quot;font-weight: bold;&quot;&gt;blf&lt;/span&gt; - Batch weight/bias learning function,&lt;br&gt;                default = &quot;learngdm&quot;&lt;br&gt;          &lt;span&#xa; style=&quot;font-weight: bold;&quot;&gt;pf&lt;/span&gt;  - Performance function,&lt;br&gt;                default = &quot;mse&quot;.&lt;br&gt;&lt;/pre&gt;&#xa;  &lt;pre class=&quot;example&quot;&gt;          EXAMPLE 1&lt;br&gt;          Pr = [0.1 0.8; 0.1 0.75; 0.01 0.8];&lt;br&gt;               it&apos;s a 3 x 2 matrix, this means 3 input neurons&lt;br&gt;          &lt;br&gt;          net = newff(Pr, [4 1], {&quot;tansig&quot;,&quot;purelin&quot;}, &quot;trainlm&quot;, &quot;learngdm&quot;, &quot;mse&quot;);&lt;br&gt;&lt;/pre&gt;&#xa;&lt;/blockquote&gt;&#xa;&lt;/div&gt;&lt;/html&gt;&#xa;"/>
      </Properties>
